{
    "name": "mantas-done/subtitles",
    "description": "Subtitle converter and generator for PHP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Done\\Subtitles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Helpers\\": "test_helpers/"
        }
    },
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "2.0",
        "phpstan/phpstan-strict-rules": "2.0"
    }
}
